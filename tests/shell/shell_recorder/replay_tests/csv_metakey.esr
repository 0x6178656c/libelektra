Storage: csvstorage
Mountpoint: user:/tests/csvstorage
MountArgs: --with-recommends
File: config.csv

RET: 0
< kdb mount $File $Mountpoint $Storage

RET: 0
< kdb set user:/tests/csvstorage/test test

RET: 0
< printf 'one,two,three\nfour,five,six\n' > `kdb file user:/tests/csvstorage`

RET: 0
< kdb ls $Mountpoint

RET: 0
< kdb meta-get user:/tests/csvstorage/#0 array

RET: 0
< kdb rm -r $Mountpoint

RET: 0
< kdb umount $Mountpoint
